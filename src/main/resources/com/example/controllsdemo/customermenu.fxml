<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="308.0" prefWidth="357.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.controllsdemo.Customermenu">
    <VBox layoutX="71.0" layoutY="30.0" prefHeight="182.0" prefWidth="243.0">
        <children>
            <HBox prefHeight="36.0" prefWidth="243.0">
