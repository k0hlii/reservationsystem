            <children>
               <Label prefHeight="17.0" prefWidth="67.0" text="Tel. Nr." />
               <TextField fx:id="inpTelNr" />
            </children>
         </HBox>
         <HBox prefHeight="36.0" prefWidth="243.0">
            <children>
               <Label prefHeight="17.0" prefWidth="67.0" text="Email" />
               <TextField fx:id="inpEmail" />
            </children>
         </HBox>
        </children>
    </VBox>
    <Button fx:id="btnSubmit" layoutX="122.0" layoutY="234.0" mnemonicParsing="false" onAction="#handlSubmit" text="OK" />
    <Button fx:id="btnCancel" layoutX="184.0" layoutY="234.0" mnemonicParsing="false" onAction="#handlebtnCancel" text="Abbrechen" />
